import java.util.Scanner; 
import java.lang.*;
import java.util.*;

public class Vitale{
	double cardiaque; //entre 40 et 200 battements par minute  moyenne : 70 bat/min
	double pressionHaute; //entre 7.0 et 25  moyenne : 12
	double pressionBasse; //entre 0 et 15  moyenne : 8
	double glycemie; //entre 0 et 2 g/l  moyenne : 0.85 g/l
	double tempCoprs; //entre 30 et 45 °C  moyenne : 37 °C
	double tauxHydrat; //entre 20 et 90 %  moyenne : 55%

	public Vitale(){
		this.cardiaque = this.Cardiaque();
		this.pressionHaute = this.PressionHaute();
		this.pressionBasse = this.PressionBasse();
		this.glycemie = this.Glycemie();
		this.tempCorps = this.TempCorps();
		this.tauxHydrat = this.TauxHydrat();


public int Cardiaque(){       //Simulation des battements de l'explorateur
	double cardiaque;
	double min;
	double moy;
	double max;								
	min = 40.0;
	moy = 70.0;
	max = 200.0;
	cardiaque =  generate(min, max, moy);
return cardiaque;
}

public int PressionHaute(){    //Simulation de la pression systolique de l'explorateur
double pressionH;
	double min;
	double moy;
	double max;												
	min = 7.0;
	moy = 12.0;
	max = 25.0;
	pressionH =  generate(min, max, moy);
return pressionH;
}

public int PressionBasse(){    //Simulation de la pression diastolique de l'explorateur
